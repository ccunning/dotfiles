!! URxvt Appearance
URxvt.font: xft:FreeMono:style=Regular:size=12
URxvt.boldFont: xft:FreeMono:style=Bold
URxvt.italicFont: xft:FreeMono:style=Oblique
URxvt.boldItalicfont: xft:FreeMono:style=Bold Oblique
URxvt.letterSpace: 0
URxvt.lineSpace: 0
URxvt.geometry: 80x60
URxvt.internalBorder: 4
URxvt.cursorBlink: true
URxvt.cursorUnderline: false
URxvt.saveline: 2048
URxvt.scrollBar: false
URxvt.scrollBar_right: false
URxvt.urgentOnBell: true
URxvt.depth: 32
URxvt.iso14755: false
URxvt*background: [95]#0c0d0e

!! URxvt config
URxvt.perl-lib: /usr/lib/urxvt/perl
URxvt.perl-ext-common: default,font-size
URxvt.keysym.Shift-Control-V: eval:paste_clipboard
URxvt.keysym.Shift-Control-C: eval:selection_to_clipboard
URxvt.keysym.Control-Meta-c: builtin-string:
URxvt.keysym.Control-Meta-v: builtin-string:

URxvt.keysym.C-plus:      font-size:increase
URxvt.keysym.C-minus:     font-size:decrease
URxvt.keysym.C-equal:     font-size:reset
URxvt.keysym.C-question:  font-size:show
URxvt.font-size.step:     4
